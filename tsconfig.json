{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "jsx": "react",
    "lib": [
      "es2018",
      "dom"
    ],
    "esModuleInterop": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": false,
  }
}
